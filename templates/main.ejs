<%- text %>
<% responses.forEach(function(response){ %>

<div class="response">
    <button onClick="setResponseCookies(<%= JSON.stringify(response) %>)" hx-get="/scene/<%= response.target %>" hx-target="#content" hx-swap="innerHTML"})"> <%= response.text %> </button>
    <% }); %>
</div>
<h2 id="header" hx-swap-oob="true">
    <%= id %>
</h2>
